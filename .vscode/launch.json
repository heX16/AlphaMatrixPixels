{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug GUI_Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/GUI_Test/bin/Debug/HXLED_GUI_Test.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/GUI_Test/bin/Debug",
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build GUI_Test (Debug)"
        },
        {
            "name": "Run GUI_Test (Release)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/GUI_Test/bin/Release/HXLED_GUI_Test.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/GUI_Test/bin/Release",
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build GUI_Test (Release)"
        }
    ]
}

